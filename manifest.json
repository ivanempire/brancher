{
	"name": "Brancher",
	"description": "GitHub branch autocompletion for BitRise",
	"version": "1.0",
	"manifest_version": 3,
	"background": {
		"service_worker": "background.js"
	},
	"permissions": ["storage"],
	"options_ui": {
		"page": "options.html",
		"open_in_tab": false
	},
	"content_scripts": [
		{
			"matches": ["https://app.bitrise.io/app/*"],
			"js": ["foreground.js"],
			"run_at": "document_idle"
		}
	]
}